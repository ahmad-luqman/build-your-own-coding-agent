{
  "name": "coding-agent",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "check": "bun run lint && bun run typecheck && bun test",
    "prepare": "bunx lefthook install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.30",
    "@inkjs/ui": "^2.0.0",
    "@openrouter/ai-sdk-provider": "^0.4.3",
    "ai": "^6.0.0-beta.128",
    "chalk": "^5.4.0",
    "cli-highlight": "^2.1.11",
    "dotenv": "^16.4.0",
    "glob": "^11.0.0",
    "ink": "^5.2.0",
    "marked": "^15.0.0",
    "marked-terminal": "^7.3.0",
    "react": "^18.3.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/bun": "^1.2.0",
    "@types/react": "^18.3.0",
    "lefthook": "^2.1.1",
    "typescript": "^5.7.0"
  }
}
